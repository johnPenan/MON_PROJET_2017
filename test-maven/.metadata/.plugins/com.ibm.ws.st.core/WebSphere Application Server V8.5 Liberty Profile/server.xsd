            <xsd:element name="ldapCache" type="com.ibm.ws.security.wim.adapter.ldap.cacheConfig"> 
                <xsd:annotation> 
                    <xsd:documentation>Configure les attributs de mémoire cache.</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Propriétés de mémoire cache</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
        </xsd:choice> 
        <xsd:attribute name="host" use="required" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Adresse du serveur LDAP sous la forme d'une adresse IP ou d'un nom DNS.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Hôte</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="port" use="required" type="intType"> 
            <xsd:annotation> 
                <xsd:documentation>Numéro de port du serveur LDAP.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Port</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="baseDN" use="required" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Nom distinctif de base du service d'annuaire, qui indique le point de démarrage des recherches LDAP dans le service d'annuaire.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Nom distinctif de base (dn)</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="ldapType" use="required"> 
            <xsd:annotation> 
                <xsd:documentation>Type de serveur LDAP auquel une connexion est établie.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Type de serveur LDAP</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:string"> 
                            <xsd:enumeration value="Microsoft Active Directory"> 
                                <xsd:annotation> 
                                    <xsd:documentation>actived</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="Custom"> 
                                <xsd:annotation> 
                                    <xsd:documentation>custom</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="IBM Lotus Domino"> 
                                <xsd:annotation> 
                                    <xsd:documentation>domino50</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="Novell eDirectory"> 
                                <xsd:annotation> 
                                    <xsd:documentation>edirectory</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="IBM Tivoli Directory Server"> 
                                <xsd:annotation> 
                                    <xsd:documentation>ibm_dir_server</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="Sun Java System Directory Server"> 
                                <xsd:annotation> 
                                    <xsd:documentation>iplanet</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="Netscape Directory Server"> 
                                <xsd:annotation> 
                                    <xsd:documentation>netscape</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="IBM SecureWay Directory Server"> 
                                <xsd:annotation> 
                                    <xsd:documentation>secureway</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="realm" use="optional" default="LdapRegistry" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Nom du superdomaine qui représente le registre d'utilisateurs.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Nom de superdomaine</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="bindDN" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Nom distinctif du serveur d'applications utilisé pour effectuer la liaison au service d'annuaire.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Nom distinctif de liaison (dn)</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="bindPassword" use="optional" type="password"> 
            <xsd:annotation> 
                <xsd:documentation>Mot de passe du nom distinctif de liaison. La valeur peut être enregistrée en texte clair ou sous forme codée. Il est recommandé de coder le mot de passe. Pour cela, utilisez l'outil securityUtility avec l'option encode.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Mot de passe de liaison</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="ignoreCase" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Effectuer une vérification d'authentification en ignorant la casse.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Ignorer la casse pour l'autorisation</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="recursiveSearch" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Spécifie si une recherche de groupes imbriqués doit être effectuée. Sélectionnez cette option uniquement si le serveur LDAP ne prend pas en charge les recherches récursives côté serveur.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Effectuer une recherche de groupes imbriqués</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="reuseConnection" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Demande au serveur d'applications de réutiliser la connexion au serveur LDAP.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Réutiliser la connexion</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="sslEnabled" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Indique si une connexion SSL doit être établie au serveur LDAP.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>LDAP SSL activé</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="sslRef" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>ID de la configuration SSL à utiliser pour établir une connexion au serveur LDAP activé par SSL.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Référence SSL</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="searchTimeout" use="optional" default="1m" type="duration"> 
            <xsd:annotation> 
                <xsd:documentation>Durée maximale pendant laquelle un serveur LDAP peut répondre avant qu'une demande ne soit annulée.  Equivalent à un dépassement du délai d'attente de lecture une fois la connexion établie. Indiquez une valeur entière positive suivie d'une unité de temps, qui peut être heure (h), minute (m), seconde (s) ou milliseconde (ms). Par exemple, pour 500 millisecondes, indiquez 500ms. Vous pouvez inclure plusieurs valeurs dans une même entrée. Par exemple, 1s500ms correspond à 1 seconde et demie.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Délai d'expiration de la recherche</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="connectTimeout" use="optional" default="1m" type="duration"> 
            <xsd:annotation> 
                <xsd:documentation>Délai maximum pour l'établissement d'une connexion au serveur LDAP. Un message d'erreur sera consigné si le délai indiqué expire. Indiquez une valeur entière positive suivie d'une unité de temps, qui peut être heure (h), minute (m), seconde (s) ou milliseconde (ms). Par exemple, pour 500 millisecondes, indiquez 500ms. Vous pouvez inclure plusieurs valeurs dans une même entrée. Par exemple, 1s500ms correspond à 1 seconde et demie.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Délai d'attente pour l'établissement d'une connexion LDAP</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="certificateMapMode" use="optional"> 
            <xsd:annotation> 
                <xsd:documentation>Indique s'il convient de mapper des certificats x.509 dans un répertoire LDAP par EXACT_DN ou CERTIFICATE_FILTER. Indiquez CERTIFICATE_FILTER pour utiliser le filtre de certificat spécifié pour le mappage.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Mode de mappage de certificat</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:string"> 
                            <xsd:enumeration value="EXACT_DN"> 
                                <xsd:annotation> 
                                    <xsd:documentation>exactDN</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="CERTIFICATE_FILTER"> 
                                <xsd:annotation> 
                                    <xsd:documentation>certFilter</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="certificateFilter" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Indique la propriété de mappage de certificat de filtre pour le filtre LDAP. Le filtre est utilisé pour mapper des attributs dans le certificat de client pour les entrées du registre LDAP. Par exemple, le filtre peut être spécifié sous la forme : uid=${SubjectCN}.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Filtre de mappe de certificat</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="activedFiltersRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>Spécifie la liste des filtres LDAP Microsoft Active Directory par défaut.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Référence des filtres LDAP Microsoft Active Directory</ext:label> 
                    <ext:reference>activedLdapFilterProperties</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="customFiltersRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>Spécifie la liste des filtres LDAP personnalisés par défaut.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Référence des filtres LDAP personnalisés</ext:label> 
                    <ext:reference>customLdapFilterProperties</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="domino50FiltersRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>Spécifie la liste des filtres LDAP IBM Lotus Domino par défaut.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Référence des filtres LDAP IBM Lotus Domino</ext:label> 
                    <ext:reference>domino50LdapFilterProperties</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="edirectoryFiltersRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>Spécifie la liste des filtres LDAP Novell eDirectory.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Référence des filtres LDAP Novell eDirectory</ext:label> 
                    <ext:reference>edirectoryLdapFilterProperties</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="idsFiltersRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>Spécifie la liste des filtres LDAP IBM Tivoli Directory Server par défaut.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Référence des filtres LDAP IBM Tivoli Directory</ext:label> 
                    <ext:reference>idsLdapFilterProperties</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="iplanetFiltersRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>Spécifie la liste des filtres LDAP Sun Java System Directory Server par défaut.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Référence des filtres LDAP Sun Java System Directory</ext:label> 
                    <ext:reference>iplanetLdapFilterProperties</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="netscapeFiltersRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>Spécifie la liste des filtres LDAP Netscape Directory Server par défaut.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Référence des filtres LDAP Netscape Directory Server</ext:label> 
                    <ext:reference>netscapeLdapFilterProperties</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="securewayFiltersRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>Spécifie la liste des filtres LDAP IBM SecureWay Directory Server par défaut.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Référence des filtres LDAP IBM SecureWay Directory</ext:label> 
                    <ext:reference>securewayLdapFilterProperties</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="returnToPrimaryServer" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Valeur booléenne qui indique si la recherche doit porter sur le serveur principal.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Revenir au serveur principal</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
