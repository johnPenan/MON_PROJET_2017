    <xsd:complexType name="com.ibm.ws.security.registry.ldap.internal.filters.eDirectory"> 
        <xsd:annotation> 
            <xsd:documentation>Spécifie la liste des filtres LDAP Novell eDirectory.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Filtres LDAP Novell eDirectory</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="userFilter" use="optional" default="(&amp;(cn=%v)(objectclass=Person))" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Définit une clause de filtrage LDAP pour rechercher des utilisateurs dans le registre d'utilisateurs.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Filtre d'utilisateurs</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="groupFilter" use="optional" default="(&amp;(cn=%v)(objectclass=groupOfNames))" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Clause de filtrage LDAP pour rechercher des groupes dans le registre d'utilisateurs.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Filtre de groupes</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="userIdMap" use="optional" default="person:cn" type="xsd:string"> 
