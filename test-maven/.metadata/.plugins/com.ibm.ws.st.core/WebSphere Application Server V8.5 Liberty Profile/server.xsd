                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="loginModuleRef" use="optional" default="hashtable,userNameAndPassword,certificate,token" type="pidListType"> 
            <xsd:annotation> 
                <xsd:documentation>Référence à l'ID d'un module de connexion JAAS.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Référence de module de connexion</ext:label> 
                    <ext:reference>jaasLoginModule</ext:reference> 
