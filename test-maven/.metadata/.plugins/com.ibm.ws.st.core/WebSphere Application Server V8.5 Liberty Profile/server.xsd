                                        <xsd:documentation>Vérifier un jeton d'identité ITTPrincipalName</xsd:documentation> 
                                    </xsd:annotation> 
                                </xsd:enumeration> 
                                <xsd:enumeration value="ITTX509CertChain"> 
                                    <xsd:annotation> 
