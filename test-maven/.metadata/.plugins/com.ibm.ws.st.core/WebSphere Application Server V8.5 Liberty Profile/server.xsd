    <xsd:complexType name="com.ibm.ws.app.management"> 
        <xsd:annotation> 
            <xsd:documentation>Propriétés qui contrôlent le comportement du gestionnaire d'application</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Gestionnaire d'application</ext:label> 
