            <xsd:documentation>Définit les propriétés d'une application Web.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Application Web</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:choice minOccurs="0" maxOccurs="unbounded"> 
            <xsd:element name="classloader" type="com.ibm.ws.classloading.classloader"> 
            </xsd:element> 
            <xsd:element name="application-bnd" type="com.ibm.ws.javaee.dd.appbnd"> 
            </xsd:element> 
        </xsd:choice> 
        <xsd:attribute name="location" use="required" type="location"> 
            <xsd:annotation> 
                <xsd:documentation>Emplacement d'une application sous forme d'un chemin absolu ou d'un chemin relatif au répertoire apps au niveau du serveur.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Emplacement</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="name" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Nom d'une application.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Nom</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="autoStart" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Indique si le serveur démarre ou non automatiquement l'application.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Démarrage automatique</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="suppressUncoveredHttpMethodWarning" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Option permettant de supprimer le message d'avertissement de méthode HTTP non couverte lors du déploiement d'application.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Suppression des avertissements de méthode HTTP non couverte</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="contextRoot" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Racine de contexte d'une application.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Racine de contexte d'application</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
