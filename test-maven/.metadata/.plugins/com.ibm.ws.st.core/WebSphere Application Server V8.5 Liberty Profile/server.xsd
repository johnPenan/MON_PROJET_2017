        <xsd:attribute name="groupMemberIdMap" use="optional" default="groupOfNames:member;groupOfUniqueNames:uniqueMember" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Définit un filtre LDAP qui identifie l'appartenance de l'utilisateur à un groupe.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Mappe des ID membre de groupe</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
