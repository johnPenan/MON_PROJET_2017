                                    </xsd:annotation> 
                                </xsd:enumeration> 
                                <xsd:enumeration value="ITTDistinguishedName"> 
                                    <xsd:annotation> 
                                        <xsd:documentation>Vérifier un jeton d'identité ITTDistinguishedName</xsd:documentation> 
                                    </xsd:annotation> 
                                </xsd:enumeration> 
                            </xsd:restriction> 
                        </xsd:simpleType> 
                    </xsd:union> 
